{
  "name": "nicky-blog",
  "description": "Nicky tries to blog",
  "version": "0.2.0",
  "author": "Nicky Meuleman",
  "repository": {
    "type": "git",
    "url": "https://github.com/NickyMeuleman/nicky-blog.git"
  },
  "bugs": {
    "url": "https://github.com/NickyMeuleman/nicky-blog/issues"
  },
  "keywords": [
    "blog",
    "gatsby",
    "web development"
  ],
  "homepage": "https://nickymeuleman.netlify.com/",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@emotion/core": "^10.0.28",
    "@nickymeuleman/gatsby-theme-blog": "^1.5.10",
    "@theme-ui/presets": "^0.3.0",
    "@theme-ui/prism": "^0.3.0",
    "apollo-boost": "^0.4.7",
    "apollo-server-lambda": "^2.11.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "faunadb": "^2.13.0",
    "gatsby": "^2.19.32",
    "gatsby-plugin-extract-schema": "^0.0.5",
    "gatsby-plugin-google-analytics": "^2.1.37",
    "gatsby-plugin-manifest": "^2.2.44",
    "gatsby-plugin-netlify": "^2.1.34",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sitemap": "^2.2.28",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-fetch": "^2.2.1",
    "netlify-lambda": "^1.6.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "theme-ui": "^0.3.1",
    "typeface-source-sans-pro": "^1.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "run-p build:lambda start:lambda build:app",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build './src/lambda' --config 'src/lambda/utils/webpack.functions.js'",
    "start": "run-p start:*",
    "start:app": "gatsby develop -H 0.0.0.0",
    "start:lambda": "netlify-lambda serve './src/lambda' --config 'src/lambda/utils/webpack.functions.js'",
    "serve": "gatsby serve",
    "dev": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write 'src/**/*.js'",
    "commit": "commit",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "http-proxy-middleware": "^1.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1"
  }
}
